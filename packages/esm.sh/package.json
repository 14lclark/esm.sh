{
  "name": "esm.sh",
  "version": "0.135.1",
  "description": "Build modern(es2015+) web apps with esm.sh",
  "type": "module",
  "main": "src/index.mjs",
  "module": "src/index.mjs",
  "types": "types/index.d.ts",
  "bin": "./bin/cli.mjs",
  "exports": {
    ".": {
      "deno": "./deno.mjs",
      "node": "./node.mjs",
      "import": "./src/index.mjs",
      "types": "./types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": [
    "./src/html-rewriter.mjs",
    "./deno.mjs",
    "./node.mjs"
  ],
  "scripts": {
    "start": "./bin/cli.mjs"
  },
  "files": [
    "bin/",
    "src/",
    "templates/",
    "types/",
    "vendor/",
    "deno.mjs",
    "index.mjs"
  ],
  "engines": {
    "node": ">=18.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https//github.com/esm-dev/esm.sh"
  },
  "license": "MIT"
}
